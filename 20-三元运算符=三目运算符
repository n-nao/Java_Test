package com.xxxxxxxxxxxsanyuanyunsuanfu;
//三元运算符==三目运算符
//公式: 条件?值1:值2
public class Demo {
    public static void main(String [] args) {
       boolean a=20<45?true:false;
       
       //可以转换成if....else语句
       boolean b;
       if(20<45) {
           b=true;
       }else {
           b=false;
       }
    }
}
